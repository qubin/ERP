			.quick-nav .qn-2{padding-left: 27px;}	
.slider {
	position: relative;
	display: block;
	height: 500px;
}

.slider .slider-nav {
	position: absolute;	
	width: 38px;
	height: 38px;
	bottom: 0;
	text-indent: -999em;
	color: #FFF;
	cursor: pointer;
	opacity: 0;
	z-index: 10;
}

.slider:hover .slider-nav {
	opacity: 0.6;
}

.slider:hover .slider-nav:hover {
	opacity: 1;
}

.slider .slider-nav.left {
	display: none;
	right: 65px;
    bottom: 10px;
	background: url(../images/login/slide_nav_bg.png) 0 0 no-repeat;
}

.slider .slider-nav.right {
	right: 20px;
	bottom: 10px;
	background: url(../images/login/slide_nav_bg.png) 0 -54px no-repeat;
}

.slider .slide-wrapper {
	display: block;
	overflow: hidden;
	height: 500px;
	position: relative;
	left: 0;
	z-index: 1;
}

.slider .slide-wrapper .slide {
	position: absolute;
	top: 0;
	left: 0;
	display: none;
	width: 100%;
	height: 500px;
}

.slider .slide-wrapper .slide.active {
	display: block;
}


.slider .slider-bottom-nav {
	position: absolute;
	bottom: -40px;
	left: 50%;
	margin-left: -122px;
	display: block;
	width: 244px;
	height: 20px;
	background: url(../images/login/sp.png) 0 -388px no-repeat;
}

.slider .slider-bottom-nav span {
	float: left;
	display: inline;
	width: 18px;
	height: 18px;
	font-size: 0;
	overflow: hidden;
	border-radius: 50%;
	cursor: pointer;
	margin-right: 43px;
	transition: background 200ms;
	-webkit-transition: background 200ms;
	-moz-transition: background 200ms;
	-o-transition: background 200ms;
}

.slider .slider-bottom-nav span.active {
	background-color: #ccc;
}
.lte9 .slider .slider-bottom-nav span.active {background: url(../images/login/sp.png) -279px -389px no-repeat;}

