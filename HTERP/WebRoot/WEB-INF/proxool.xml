<?xml version="1.0" encoding="UTF-8"?>
<something-else-entirely>
	<proxool>
	    <alias>MySql5</alias>
	    <driver-url>jdbc:mysql://192.168.0.100:3306/hterp</driver-url>
	    <driver-class>com.mysql.jdbc.Driver</driver-class>
	    <driver-properties>
	      	<property name="user" value="root"/>
	      	<property name="password" value="root"/>
 	        <property name="characterEncoding" value="UTF8"/>
	    </driver-properties>
	    <!-- 连接池中可用的连接数量 -->
	    <prototype-count>20</prototype-count>
	    <!-- 最大活动时间 -->
	    <maximum-active-time>3600000</maximum-active-time>
	    <!-- 最大连接生命周期 -->
	    <maximum-connection-lifetime>200</maximum-connection-lifetime>
	    <!-- 最小连接数 -->
	    <minimum-connection-count>10</minimum-connection-count>
	    <!-- 最大连接数 -->
	    <maximum-connection-count>200</maximum-connection-count>
	</proxool> 
</something-else-entirely>