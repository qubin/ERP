<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="matsoflowManager" extends="struts-default" namespace="/admin/matsoflow" >
		<action name="matsoflow_*" class="cn.joymates.erp.action.stock.MatsoflowAction" method="{1}">
            <result name="home">/WEB-INF/matsoflow/matsoflow_home.jsp</result>
            <result name="addUI">/WEB-INF/matsoflow/matsoflow_add.jsp</result>
            <result name="modifyUI">/WEB-INF/matsoflow/matsoflow_modify.jsp</result>
        </action>
	</package>
      
    <package name="stockalarmManager" extends="struts-default" namespace="/admin/stockalarm" >
		<action name="stockalarm_*" class="cn.joymates.erp.action.stock.StockAlarmAction" method="{1}">
            <result name="home">/WEB-INF/stockalarm/stockalarm_home.jsp</result>
            <result name="addUI">/WEB-INF/stockalarm/stockalarm_add.jsp</result>
            <result name="modifyUI">/WEB-INF/stockalarm/stockalarm_modify.jsp</result>
        </action>
	</package>
      
</struts>
