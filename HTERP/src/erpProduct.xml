<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="productManager" extends="struts-default" namespace="/admin/product" >
		<action name="product_*" class="cn.joymates.erp.action.baseinfo.ProductAction" method="{1}">
            <result name="home">/WEB-INF/product/product_home.jsp</result>
            <result name="addUI">/WEB-INF/product/product_add.jsp</result>
            <result name="modifyUI">/WEB-INF/product/product_modify.jsp</result>
            <result name="detailUI">/WEB-INF/product/product_detail.jsp</result>
        </action>
	</package>
      
</struts>
